{
  "expo": {
    "name": "NutriSnap",
    "slug": "forma",
    "scheme": [
      "com.raymondzhao3000.forma",
      "forma",
      "com.googleusercontent.apps.311242226872-4jrv4kndh6j0s974u1h0uqbj4bvmp3af"
    ],
    "version": "1.0.12",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.raymondzhao3000.forma",
      "googleServicesFile": "./ios/NutriSnap/GoogleService-Info.plist",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSHealthShareUsageDescription": "NutriSnap needs access to your health data to sync weight, calories, and nutrition information with Apple Health.",
        "NSHealthUpdateUsageDescription": "NutriSnap needs permission to write your weight, calories, and nutrition data to Apple Health."
      },
      "entitlements": {
        "com.apple.developer.healthkit": true,
        "com.apple.developer.healthkit.access": []
      },
      "buildNumber": "72"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.BODY_SENSORS",
        "android.permission.ACTIVITY_RECOGNITION"
      ],
      "package": "com.raymondzhao3000.forma",
      "versionCode": 6,
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "./plugins/withRemoveAdId",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow NutriSnap to access your camera to scan food photos and barcodes for your nutrition tracking."
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "Allow NutriSnap to access your microphone to record voice logs for quick and easy meal tracking."
        }
      ],
      "expo-web-browser",
      "expo-file-system",
      "expo-notifications",
      "expo-apple-authentication",
      [
        "@kingstinct/react-native-healthkit",
        {
          "NSHealthShareUsageDescription": "NutriSnap needs access to read your health data to sync weight, calories, and nutrition information with your health dashboard.",
          "NSHealthUpdateUsageDescription": "NutriSnap needs permission to write your weight, calories, and nutrition data to Apple Health to keep your health records up to date.",
          "requiredBackgroundModes": []
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.311242226872-4jrv4kndh6j0s974u1h0uqbj4bvmp3af"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "58b82cb0-192d-4a58-b0ba-3c2a0adbe514"
      }
    }
  }
}